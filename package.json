{
  "name": "publication2023",
  "type": "module",
  "version": "2.4.2",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:strict": "astro check && tsc --noEmit && vue-tsc --noEmit && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typechecking": "astro check && tsc --noEmit && vue-tsc --noEmit",
    "lint:js": "eslint --ext \".ts,.js,.astro,.vue\" --ignore-path .gitignore ./src",
    "lint:style": "stylelint \"**/*.{scss,sass}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "format": "npx prettier --write .",
    "postbuild": "pagefind --site dist/"
  },
  "dependencies": {
    "@astrojs/mdx": "^1.1.2",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/sitemap": "^3.0.2",
    "@astrojs/vue": "^3.0.1",
    "@felix_berlin/sass-butler": "^2.6.0",
    "@pagefind/default-ui": "^1.0.3",
    "@vueuse/core": "^10.5.0",
    "astro": "^3.3.2",
    "astro-breadcrumbs": "^1.8.0",
    "astro-icon": "^0.8.1",
    "astro-imagetools": "^0.9.0",
    "astrojs-service-worker": "^1.1.0",
    "lucide-vue-next": "^0.288.0",
    "pagefind": "^1.0.3",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-slug": "^6.0.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.2.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^9.2.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "sass": "^1.69.4",
    "semantic-release": "^22.0.5",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}

{
  "name": "publication2023",
  "type": "module",
  "version": "1.1.0",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && vue-tsc --noEmit && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typechecking": "astro check && tsc --noEmit && vue-tsc --noEmit",
    "lint:js": "eslint --ext \".ts,.js,.astro,.vue\" --ignore-path .gitignore ./src",
    "lint:style": "stylelint \"**/*.{scss,sass}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "format": "npx prettier --write ."
  },
  "dependencies": {
    "@astrojs/mdx": "^0.19.7",
    "@astrojs/prefetch": "^0.3.0",
    "@astrojs/sitemap": "^1.4.0",
    "@astrojs/vue": "^2.2.1",
    "@felix_berlin/sass-butler": "^2.3.0",
    "@vueuse/core": "^10.2.1",
    "astro": "^2.8.3",
    "astro-breadcrumbs": "^1.6.1",
    "astro-critters": "^1.1.39",
    "astro-icon": "^0.8.1",
    "astro-imagetools": "^0.8.1",
    "astro-pagefind": "^1.2.1",
    "astrojs-service-worker": "^1.0.0",
    "lucide-vue-next": "^0.260.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.33.0",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-astro": "^0.27.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-vue": "^9.15.1",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.26",
    "postcss-html": "^1.5.0",
    "postcss-preset-env": "^9.0.0",
    "postcss-scss": "^4.0.6",
    "prettier": "3.0.0",
    "prettier-plugin-astro": "^0.11.0",
    "sass": "^1.63.6",
    "semantic-release": "^21.0.7",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.5"
  }
}
